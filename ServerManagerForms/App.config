<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7" />
  </startup>
  <appSettings>

    <!-- Must be changed to the folder location of the EXE of the server executable. -->
    <add key="SocketServerLocation" value="E:\Proj\PlanningPoker\PokerServer\bin\Debug" />

    <!-- Name of the EXE, without '.exe' -->
    <add key="SocketServerName" value="PokerServer" />

    <!-- IP of the computer / host that the servers get run on. This gets sent to the client, and they connect via WebSockets using ws://<MachineIp>/.  -->
    <add key="MachineIp" value="169.254.123.121" />



    <!-- Must be changed to the root folder location of the PokerWebClient project.-->
    <add key="WebSiteLocation" value="E:\Proj\PlanningPoker\PokerWebClient" />

    <!-- Must be changed to the url on which Poker web client will be hosted.-->
    <add key="HostingUrl" value="http://localhost:5000/" />

    <!-- Must be changed to the default file name to be loaded when no file name is specified. -->
    <add key="DefaultFile" value="index.html" />

    <!-- Trigger to return ServerList from web host; this should macth with RoomListTrigger variable in CardClient.js; should start with /. -->
    <add key="RoomListTrigger" value="/getroomlist" />

  </appSettings>
</configuration>